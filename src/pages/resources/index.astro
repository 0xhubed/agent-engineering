---

<BaseLayout title="Resources">
  <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-16">
    <div class="mb-12">
      <h1 class="text-3xl font-bold text-slate-900 dark:text-white mb-4 tracking-tight">
        Resources
      </h1>
      <p class="text-slate-600 dark:text-slate-400 max-w-2xl">
        Curated research papers, frameworks, and tools for understanding and building AI agents.
      </p>
    </div>

    <!-- Quick Navigation -->
    <nav class="mb-12 flex flex-wrap gap-6 text-sm">
      {news.length > 0 && (
        <a href="#news" class="text-slate-500 dark:text-slate-400 hover:text-slate-900 dark:hover:text-white transition-colors">News</a>
      )}
      {hasDiscoveredResources && (
        <a href="#discovered" class="text-slate-500 dark:text-slate-400 hover:text-slate-900 dark:hover:text-white transition-colors">Discovered</a>
      )}
      <a href="#papers" class="text-slate-500 dark:text-slate-400 hover:text-slate-900 dark:hover:text-white transition-colors">Papers</a>
      <a href="#frameworks" class="text-slate-500 dark:text-slate-400 hover:text-slate-900 dark:hover:text-white transition-colors">Frameworks</a>
      <a href="#tools" class="text-slate-500 dark:text-slate-400 hover:text-slate-900 dark:hover:text-white transition-colors">Tools</a>
    </nav>

    <!-- News Section (Auto-updated daily) -->
    {news.length > 0 && (
      <section id="news" class="mb-16">
        <div class="flex items-center justify-between mb-8">
          <h2 class="text-2xl font-semibold text-slate-900 dark:text-white">
            Latest News
          </h2>
          {lastUpdated && (
            <span class="text-sm text-slate-500 dark:text-slate-400">
              Updated {lastUpdated}
            </span>
          )}
        </div>

        <div class="grid gap-4 md:grid-cols-2 lg:grid-cols-3">
          {news.slice(0, 9).map((item) => (
            <a
              href={item.url}
              target="_blank"
              rel="noopener noreferrer"
              class="block p-5 bg-white dark:bg-slate-800 rounded-lg border border-slate-200 dark:border-slate-700 hover:border-primary-300 dark:hover:border-primary-700 transition-colors"
            >
              <div class="flex items-start justify-between gap-2 mb-2">
                <h3 class="font-semibold text-slate-900 dark:text-white text-sm leading-tight">
                  {item.title}
                </h3>
                <svg class="w-4 h-4 text-slate-400 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14"/>
                </svg>
              </div>
              <p class="text-xs text-slate-500 dark:text-slate-400 mb-2">
                {item.source}
                {item.published && (
                  <span>
                    {' '}&middot;{' '}
                    {new Date(item.published).toLocaleDateString('en-US', { month: 'short', day: 'numeric' })}
                  </span>
                )}
              </p>
              <p class="text-sm text-slate-600 dark:text-slate-300 mb-3">
                {item.summary}
              </p>
              {item.tags && (
                <div class="flex flex-wrap gap-1">
                  {item.tags.map((tag) => (
                    <span class={`text-xs px-2 py-0.5 rounded-full ${tagColors[tag] || 'bg-slate-100 text-slate-800 dark:bg-slate-700 dark:text-slate-200'}`}>
                      {tag}
                    </span>
                  ))}
                </div>
              )}
            </a>
          ))}
        </div>
      </section>
    )}

    <!-- Auto-Discovered Resources Section -->
    {hasDiscoveredResources && (
      <section id="discovered" class="mb-16">
        <div class="flex items-center justify-between mb-8">
          <div>
            <h2 class="text-2xl font-semibold text-slate-900 dark:text-white">
              Recently Discovered
            </h2>
            <p class="text-sm text-slate-500 dark:text-slate-400 mt-1">
              Auto-curated from research scans
            </p>
          </div>
          {resourcesLastUpdated && (
            <span class="text-sm text-slate-500 dark:text-slate-400">
              Updated {resourcesLastUpdated}
            </span>
          )}
        </div>

        {/* Discovered Papers */}
        {discoveredPapers.length > 0 && (
          <div class="mb-8">
            <h3 class="text-sm font-medium text-slate-500 dark:text-slate-400 uppercase tracking-wider mb-4">
              Papers
            </h3>
            <div class="grid gap-4 md:grid-cols-2">
              {discoveredPapers.slice(0, 6).map((paper) => (
                <a
                  href={paper.url}
                  target="_blank"
                  rel="noopener noreferrer"
                  class="block p-5 bg-white dark:bg-slate-800 rounded-lg border border-slate-200 dark:border-slate-700 hover:border-primary-300 dark:hover:border-primary-700 transition-colors"
                >
                  <h4 class="font-semibold text-slate-900 dark:text-white mb-1 flex items-start gap-2">
                    <span class="flex-1">{paper.title}</span>
                    <svg class="w-4 h-4 text-slate-400 flex-shrink-0 mt-0.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14"/>
                    </svg>
                  </h4>
                  <p class="text-xs text-slate-500 dark:text-slate-400 mb-2">
                    {paper.source} {paper.metadata?.year && `(${paper.metadata.year})`}
                  </p>
                  <p class="text-sm text-slate-600 dark:text-slate-300 mb-2">
                    {paper.description}
                  </p>
                  {paper.topics.length > 0 && (
                    <div class="flex flex-wrap gap-1">
                      {paper.topics.slice(0, 3).map((topic) => (
                        <span class={`text-xs px-2 py-0.5 rounded-full ${tagColors[topic] || 'bg-slate-100 text-slate-800 dark:bg-slate-700 dark:text-slate-200'}`}>
                          {topic}
                        </span>
                      ))}
                    </div>
                  )}
                </a>
              ))}
            </div>
          </div>
        )}

        {/* Discovered Repos */}
        {discoveredRepos.length > 0 && (
          <div class="mb-8">
            <h3 class="text-sm font-medium text-slate-500 dark:text-slate-400 uppercase tracking-wider mb-4">
              Repositories
            </h3>
            <div class="grid gap-4 md:grid-cols-2 lg:grid-cols-3">
              {discoveredRepos.slice(0, 6).map((repo) => (
                <a
                  href={repo.url}
                  target="_blank"
                  rel="noopener noreferrer"
                  class="block p-5 bg-white dark:bg-slate-800 rounded-lg border border-slate-200 dark:border-slate-700 hover:border-primary-300 dark:hover:border-primary-700 transition-colors"
                >
                  <h4 class="font-semibold text-slate-900 dark:text-white mb-1 flex items-center gap-2">
                    <span class="flex-1 truncate">{repo.title}</span>
                    <svg class="w-4 h-4 text-slate-400 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14"/>
                    </svg>
                  </h4>
                  <p class="text-xs text-slate-500 dark:text-slate-400 mb-2">
                    {repo.metadata?.language && <span>{repo.metadata.language}</span>}
                    {repo.metadata?.stars && <span> Â· {repo.metadata.stars.toLocaleString()} stars</span>}
                  </p>
                  <p class="text-sm text-slate-600 dark:text-slate-300">
                    {repo.description}
                  </p>
                </a>
              ))}
            </div>
          </div>
        )}

        {/* Discovered Tutorials */}
        {discoveredTutorials.length > 0 && (
          <div class="mb-8">
            <h3 class="text-sm font-medium text-slate-500 dark:text-slate-400 uppercase tracking-wider mb-4">
              Tutorials & Videos
            </h3>
            <div class="grid gap-4 md:grid-cols-2 lg:grid-cols-3">
              {discoveredTutorials.slice(0, 6).map((tutorial) => (
                <a
                  href={tutorial.url}
                  target="_blank"
                  rel="noopener noreferrer"
                  class="block p-5 bg-white dark:bg-slate-800 rounded-lg border border-slate-200 dark:border-slate-700 hover:border-primary-300 dark:hover:border-primary-700 transition-colors"
                >
                  <h4 class="font-semibold text-slate-900 dark:text-white mb-1 flex items-center gap-2">
                    <span class="flex-1">{tutorial.title}</span>
                    <svg class="w-4 h-4 text-slate-400 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14"/>
                    </svg>
                  </h4>
                  <p class="text-xs text-slate-500 dark:text-slate-400 mb-2">
                    {tutorial.metadata?.channel || tutorial.source}
                  </p>
                  <p class="text-sm text-slate-600 dark:text-slate-300">
                    {tutorial.description}
                  </p>
                </a>
              ))}
            </div>
          </div>
        )}

        {/* Discovered Articles */}
        {discoveredArticles.length > 0 && (
          <div class="mb-8">
            <h3 class="text-sm font-medium text-slate-500 dark:text-slate-400 uppercase tracking-wider mb-4">
              Articles
            </h3>
            <div class="grid gap-4 md:grid-cols-2 lg:grid-cols-3">
              {discoveredArticles.slice(0, 6).map((article) => (
                <a
                  href={article.url}
                  target="_blank"
                  rel="noopener noreferrer"
