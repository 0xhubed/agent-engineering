    {
      title: 'Red Teaming Language Models with Language Models',
      authors: 'Perez et al.',
      year: 2022,
      venue: 'EMNLP 2022',
      description: 'Automated red teaming for safety evaluation.',
      url: 'https://arxiv.org/abs/2202.03286',
    },
  ],
};

// Frameworks organized by category
const frameworks = {
  orchestration: [
    {
      name: 'LangGraph',
      description: 'Library for building stateful, multi-actor applications with LLMs. Graph-based control flow.',
      url: 'https://langchain-ai.github.io/langgraph/',
