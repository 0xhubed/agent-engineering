---
import BaseLayout from '../../layouts/BaseLayout.astro';

// Type for news items
interface NewsItem {
  title: string;
  url: string;
  summary: string;
  published: string | null;
  source: string;
  category: string;
  tags?: string[];
  processed_at?: string;
}

interface NewsData {
  last_updated: string | null;
  items: NewsItem[];
}

// Import news data (auto-updated daily by GitHub Actions)
import newsDataRaw from '../../data/news.json';
const newsData = newsDataRaw as NewsData;

const news: NewsItem[] = newsData.items || [];

// Type for auto-discovered resources
interface DiscoveredResource {
  title: string;
  url: string;
  description: string;
  category: string;
  source: string;
  type: string;
  topics: string[];
  score: number;
  discovered_at: string;
  metadata?: {
    arxiv_id?: string;
    year?: number;
    stars?: number;
    language?: string;
    channel?: string;
  };
}

interface ResourcesData {
  last_updated: string | null;
  papers: DiscoveredResource[];
  repos: DiscoveredResource[];
  tutorials: DiscoveredResource[];
  articles: DiscoveredResource[];
}

// Import auto-discovered resources (updated weekly by GitHub Actions)
import resourcesDataRaw from '../../data/resources.json';
const resourcesData = resourcesDataRaw as ResourcesData;

const discoveredPapers = resourcesData.papers || [];
const discoveredRepos = resourcesData.repos || [];
const discoveredTutorials = resourcesData.tutorials || [];
const discoveredArticles = resourcesData.articles || [];
const hasDiscoveredResources = discoveredPapers.length > 0 || discoveredRepos.length > 0 || discoveredTutorials.length > 0 || discoveredArticles.length > 0;
const resourcesLastUpdated = resourcesData.last_updated
  ? new Date(resourcesData.last_updated).toLocaleDateString('en-US', {
      month: 'short',
      day: 'numeric',
      year: 'numeric',
    })
  : null;
const lastUpdated = newsData.last_updated
  ? new Date(newsData.last_updated).toLocaleDateString('en-US', {
      month: 'short',
      day: 'numeric',
      year: 'numeric',
    })
  : null;

// Tag colors for news items
const tagColors: Record<string, string> = {
  frameworks: 'bg-blue-100 text-blue-800 dark:bg-blue-900 dark:text-blue-200',
  models: 'bg-purple-100 text-purple-800 dark:bg-purple-900 dark:text-purple-200',
  protocols: 'bg-green-100 text-green-800 dark:bg-green-900 dark:text-green-200',
  patterns: 'bg-yellow-100 text-yellow-800 dark:bg-yellow-900 dark:text-yellow-200',
  rag: 'bg-orange-100 text-orange-800 dark:bg-orange-900 dark:text-orange-200',
  evaluation: 'bg-red-100 text-red-800 dark:bg-red-900 dark:text-red-200',
  safety: 'bg-pink-100 text-pink-800 dark:bg-pink-900 dark:text-pink-200',
  tools: 'bg-indigo-100 text-indigo-800 dark:bg-indigo-900 dark:text-indigo-200',
  research: 'bg-teal-100 text-teal-800 dark:bg-teal-900 dark:text-teal-200',
};

// Research papers organized by topic
const papers = {
  foundational: [
    {
      title: 'ReAct: Synergizing Reasoning and Acting in Language Models',
      authors: 'Yao et al.',
      year: 2023,
      venue: 'ICLR 2023',
      description: 'Introduces the ReAct paradigm combining reasoning traces with actions.',
      url: 'https://arxiv.org/abs/2210.03629',
    },
    {
      title: 'Toolformer: Language Models Can Teach Themselves to Use Tools',
      authors: 'Schick et al.',
      year: 2023,
      venue: 'NeurIPS 2023',
      description: 'Demonstrates self-supervised tool use learning in LLMs.',
      url: 'https://arxiv.org/abs/2302.04761',
    },
    {
      title: 'Chain-of-Thought Prompting Elicits Reasoning in Large Language Models',
      authors: 'Wei et al.',
