---
import BaseLayout from '../../layouts/BaseLayout.astro';
import Callout from '../../components/Callout.astro';

// Research papers organized by topic
const papers = {
  foundational: [
    {
      title: 'ReAct: Synergizing Reasoning and Acting in Language Models',
      authors: 'Yao et al.',
      year: 2023,
      venue: 'ICLR 2023',
      description: 'Introduces the ReAct paradigm combining reasoning traces with actions.',
      url: 'https://arxiv.org/abs/2210.03629',
    },
    {
      title: 'Toolformer: Language Models Can Teach Themselves to Use Tools',
      authors: 'Schick et al.',
      year: 2023,
      venue: 'NeurIPS 2023',
      description: 'Demonstrates self-supervised tool use learning in LLMs.',
      url: 'https://arxiv.org/abs/2302.04761',
    },
    {
      title: 'Chain-of-Thought Prompting Elicits Reasoning in Large Language Models',
      authors: 'Wei et al.',
      year: 2022,
      venue: 'NeurIPS 2022',
      description: 'Foundational work on prompting LLMs for step-by-step reasoning.',
      url: 'https://arxiv.org/abs/2201.11903',
    },
    {
      title: 'Tree of Thoughts: Deliberate Problem Solving with Large Language Models',
      authors: 'Yao et al.',
      year: 2023,
      venue: 'NeurIPS 2023',
      description: 'Extends CoT with exploration of multiple reasoning paths.',
      url: 'https://arxiv.org/abs/2305.10601',
    },
  ],
  memory: [
    {
      title: 'Generative Agents: Interactive Simulacra of Human Behavior',
      authors: 'Park et al.',
      year: 2023,
      venue: 'UIST 2023',
      description: 'Agents with memory for believable social simulation.',
      url: 'https://arxiv.org/abs/2304.03442',
    },
    {
      title: 'MemGPT: Towards LLMs as Operating Systems',
      authors: 'Packer et al.',
      year: 2023,
      venue: 'arXiv',
      description: 'Hierarchical memory management for unbounded context.',
      url: 'https://arxiv.org/abs/2310.08560',
    },
    {
      title: 'Reflexion: Language Agents with Verbal Reinforcement Learning',
      authors: 'Shinn et al.',
      year: 2023,
      venue: 'NeurIPS 2023',
      description: 'Agents that learn from self-reflection and memory.',
      url: 'https://arxiv.org/abs/2303.11366',
    },
  ],
  multiAgent: [
    {
      title: 'AutoGen: Enabling Next-Gen LLM Applications via Multi-Agent Conversation',
      authors: 'Wu et al.',
      year: 2023,
      venue: 'arXiv',
      description: 'Framework for multi-agent conversation and collaboration.',
      url: 'https://arxiv.org/abs/2308.08155',
    },
    {
      title: 'MetaGPT: Meta Programming for A Multi-Agent Collaborative Framework',
      authors: 'Hong et al.',
      year: 2023,
      venue: 'arXiv',
      description: 'Role-based multi-agent system for software development.',
      url: 'https://arxiv.org/abs/2308.00352',
    },
    {
      title: 'Encouraging Divergent Thinking in Large Language Models through Multi-Agent Debate',
      authors: 'Liang et al.',
      year: 2023,
      venue: 'arXiv',
      description: 'Multiple agents debate to improve reasoning quality.',
      url: 'https://arxiv.org/abs/2305.19118',
    },
  ],
  rag: [
    {
      title: 'Retrieval-Augmented Generation for Knowledge-Intensive NLP Tasks',
      authors: 'Lewis et al.',
      year: 2020,
      venue: 'NeurIPS 2020',
      description: 'Original RAG paper combining retrieval with generation.',
      url: 'https://arxiv.org/abs/2005.11401',
    },
    {
      title: 'Self-RAG: Learning to Retrieve, Generate, and Critique through Self-Reflection',
      authors: 'Asai et al.',
      year: 2023,
      venue: 'arXiv',
      description: 'Agents that decide when and what to retrieve.',
      url: 'https://arxiv.org/abs/2310.11511',
    },
    {
      title: 'Corrective Retrieval Augmented Generation',
      authors: 'Yan et al.',
      year: 2024,
      venue: 'arXiv',
      description: 'Self-correcting retrieval with web search fallback.',
      url: 'https://arxiv.org/abs/2401.15884',
    },
    {
      title: 'From Local to Global: A Graph RAG Approach to Query-Focused Summarization',
      authors: 'Edge et al.',
      year: 2024,
      venue: 'arXiv',
      description: 'Knowledge graph-based RAG for complex queries.',
      url: 'https://arxiv.org/abs/2404.16130',
    },
  ],
  benchmarks: [
    {
      title: 'SWE-bench: Can Language Models Resolve Real-World GitHub Issues?',
      authors: 'Jimenez et al.',
      year: 2024,
      venue: 'ICLR 2024',
      description: 'Benchmark for evaluating code agents on real issues.',
      url: 'https://arxiv.org/abs/2310.06770',
    },
    {
      title: 'WebArena: A Realistic Web Environment for Building Autonomous Agents',
      authors: 'Zhou et al.',
      year: 2024,
      venue: 'ICLR 2024',
      description: 'Benchmark for web navigation and interaction.',
      url: 'https://arxiv.org/abs/2307.13854',
    },
    {
      title: 'GAIA: A Benchmark for General AI Assistants',
      authors: 'Mialon et al.',
      year: 2023,
      venue: 'arXiv',
      description: 'Multi-step reasoning benchmark for AI assistants.',
      url: 'https://arxiv.org/abs/2311.12983',
    },
    {
      title: 'AgentBench: Evaluating LLMs as Agents',
      authors: 'Liu et al.',
      year: 2023,
      venue: 'arXiv',
      description: 'Multi-environment evaluation of agent capabilities.',
      url: 'https://arxiv.org/abs/2308.03688',
    },
  ],
  safety: [
    {
      title: 'Constitutional AI: Harmlessness from AI Feedback',
      authors: 'Bai et al.',
      year: 2022,
      venue: 'arXiv',
      description: 'Self-supervision for safe AI behavior.',
      url: 'https://arxiv.org/abs/2212.08073',
    },
    {
      title: 'Red Teaming Language Models with Language Models',
      authors: 'Perez et al.',
      year: 2022,
      venue: 'EMNLP 2022',
      description: 'Automated red teaming for safety evaluation.',
      url: 'https://arxiv.org/abs/2202.03286',
    },
  ],
};

// Frameworks organized by category
const frameworks = {
  orchestration: [
    {
      name: 'LangGraph',
      description: 'Library for building stateful, multi-actor applications with LLMs. Graph-based control flow.',
      url: 'https://langchain-ai.github.io/langgraph/',
      language: 'Python',
    },
    {
      name: 'AutoGen',
      description: 'Microsoft framework for multi-agent conversations. Supports diverse agent types.',
      url: 'https://microsoft.github.io/autogen/',
      language: 'Python',
    },
    {
      name: 'CrewAI',
      description: 'Framework for orchestrating role-playing AI agents. Focus on collaboration.',
      url: 'https://www.crewai.com/',
      language: 'Python',
    },
    {
      name: 'Semantic Kernel',
      description: 'Microsoft SDK for building AI agents. Multi-language support.',
      url: 'https://github.com/microsoft/semantic-kernel',
      language: 'Python/C#/Java',
    },
  ],
  tooling: [
    {
      name: 'LangChain',
      description: 'Framework for developing LLM-powered applications. Large ecosystem.',
      url: 'https://langchain.com',
      language: 'Python/JS',
    },
    {
      name: 'LlamaIndex',
      description: 'Data framework for LLM applications. Focus on RAG and data ingestion.',
      url: 'https://www.llamaindex.ai/',
      language: 'Python',
    },
    {
      name: 'Haystack',
      description: 'End-to-end NLP framework. Production-ready pipelines.',
      url: 'https://haystack.deepset.ai/',
      language: 'Python',
    },
  ],
  evaluation: [
    {
      name: 'DeepEval',
      description: 'Open-source evaluation framework for LLMs. Agent-specific metrics.',
      url: 'https://docs.confident-ai.com/',
      language: 'Python',
    },
    {
      name: 'RAGAS',
      description: 'Evaluation framework for RAG applications. Component-level metrics.',
      url: 'https://docs.ragas.io/',
      language: 'Python',
    },
    {
      name: 'Promptfoo',
      description: 'CLI tool for testing and evaluating prompts. CI/CD integration.',
      url: 'https://www.promptfoo.dev/',
      language: 'Node.js',
    },
  ],
  protocols: [
    {
      name: 'Model Context Protocol (MCP)',
      description: 'Anthropic protocol for connecting AI with tools and data sources.',
      url: 'https://modelcontextprotocol.io/',
      language: 'Multi-language',
    },
    {
      name: 'Agent2Agent Protocol (A2A)',
      description: 'Google protocol for agent-to-agent communication and discovery.',
      url: 'https://google.github.io/A2A/',
      language: 'Multi-language',
    },
  ],
};

// Tools organized by category
const tools = {
  development: [
    {
      name: 'Claude Code',
      description: 'Anthropic CLI for AI-assisted software development with full codebase context.',
      url: 'https://docs.anthropic.com/claude-code',
    },
    {
      name: 'Cursor',
      description: 'AI-first code editor with integrated agent capabilities and multi-file editing.',
      url: 'https://cursor.sh',
    },
    {
      name: 'GitHub Copilot',
      description: 'AI pair programmer for code suggestions, chat, and workspace understanding.',
      url: 'https://github.com/features/copilot',
    },
    {
      name: 'Cody',
      description: 'Sourcegraph AI coding assistant with codebase-aware context.',
      url: 'https://sourcegraph.com/cody',
    },
    {
      name: 'Aider',
      description: 'Open-source AI pair programming in your terminal. Git-aware.',
      url: 'https://aider.chat',
    },
  ],
  automation: [
    {
      name: 'Devin',
      description: 'Autonomous AI software engineer. End-to-end development.',
      url: 'https://www.cognition-labs.com/devin',
    },
    {
      name: 'OpenHands',
      description: 'Open-source AI software development platform. Research-focused.',
      url: 'https://github.com/All-Hands-AI/OpenHands',
    },
    {
      name: 'SWE-agent',
      description: 'Princeton agent for SWE-bench. Open-source reference implementation.',
      url: 'https://github.com/princeton-nlp/SWE-agent',
    },
  ],
  observability: [
    {
      name: 'LangSmith',
      description: 'Platform for debugging, testing, and monitoring LLM applications.',
      url: 'https://smith.langchain.com/',
    },
    {
      name: 'Weights & Biases',
      description: 'ML experiment tracking with LLM evaluation features.',
      url: 'https://wandb.ai/',
    },
    {
      name: 'Braintrust',
      description: 'Enterprise platform for AI product development. Evals and logging.',
      url: 'https://braintrustdata.com/',
    },
  ],
};
---

<BaseLayout title="Resources">
  <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-12">
    <div class="mb-12">
      <h1 class="text-3xl sm:text-4xl font-bold text-slate-900 dark:text-white mb-4">
        Resources
      </h1>
      <p class="text-lg text-slate-600 dark:text-slate-300 max-w-3xl">
        Curated research papers, frameworks, and tools for understanding and building AI agents. Updated regularly with the latest developments.
      </p>
    </div>

    <!-- Quick Navigation -->
    <nav class="mb-12 p-4 bg-slate-50 dark:bg-slate-800/50 rounded-lg">
      <div class="flex flex-wrap gap-4 text-sm">
        <a href="#papers" class="text-primary-600 dark:text-primary-400 hover:underline">Research Papers</a>
        <span class="text-slate-300 dark:text-slate-600">|</span>
        <a href="#frameworks" class="text-primary-600 dark:text-primary-400 hover:underline">Frameworks</a>
        <span class="text-slate-300 dark:text-slate-600">|</span>
        <a href="#tools" class="text-primary-600 dark:text-primary-400 hover:underline">Tools</a>
      </div>
    </nav>

    <!-- Research Papers Section -->
    <section id="papers" class="mb-16">
      <h2 class="text-2xl font-bold text-slate-900 dark:text-white mb-8">
        Research Papers
      </h2>

      <!-- Foundational Papers -->
      <div class="mb-10">
        <h3 class="text-xl font-semibold text-slate-800 dark:text-slate-200 mb-4 flex items-center gap-2">
          <span class="w-2 h-2 rounded-full bg-primary-500"></span>
          Foundational
        </h3>
        <div class="grid gap-4 md:grid-cols-2">
          {papers.foundational.map((paper) => (
            <a
              href={paper.url}
              target="_blank"
              rel="noopener noreferrer"
              class="block p-5 bg-white dark:bg-slate-800 rounded-lg border border-slate-200 dark:border-slate-700 hover:border-primary-300 dark:hover:border-primary-700 transition-colors"
            >
              <h4 class="font-semibold text-slate-900 dark:text-white mb-1 flex items-start gap-2">
                {paper.title}
                <svg class="w-4 h-4 text-slate-400 flex-shrink-0 mt-0.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14"/>
                </svg>
              </h4>
              <p class="text-sm text-slate-500 dark:text-slate-400 mb-2">
                {paper.authors} ({paper.year}) &middot; {paper.venue}
              </p>
              <p class="text-sm text-slate-600 dark:text-slate-300">
                {paper.description}
              </p>
            </a>
          ))}
        </div>
      </div>

      <!-- Memory Papers -->
      <div class="mb-10">
        <h3 class="text-xl font-semibold text-slate-800 dark:text-slate-200 mb-4 flex items-center gap-2">
          <span class="w-2 h-2 rounded-full bg-primary-500"></span>
          Memory & Learning
        </h3>
        <div class="grid gap-4 md:grid-cols-2">
          {papers.memory.map((paper) => (
            <a
              href={paper.url}
              target="_blank"
              rel="noopener noreferrer"
              class="block p-5 bg-white dark:bg-slate-800 rounded-lg border border-slate-200 dark:border-slate-700 hover:border-primary-300 dark:hover:border-primary-700 transition-colors"
            >
              <h4 class="font-semibold text-slate-900 dark:text-white mb-1 flex items-start gap-2">
                {paper.title}
                <svg class="w-4 h-4 text-slate-400 flex-shrink-0 mt-0.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14"/>
                </svg>
              </h4>
              <p class="text-sm text-slate-500 dark:text-slate-400 mb-2">
                {paper.authors} ({paper.year}) &middot; {paper.venue}
              </p>
              <p class="text-sm text-slate-600 dark:text-slate-300">
                {paper.description}
              </p>
            </a>
          ))}
        </div>
      </div>

      <!-- Multi-Agent Papers -->
      <div class="mb-10">
        <h3 class="text-xl font-semibold text-slate-800 dark:text-slate-200 mb-4 flex items-center gap-2">
          <span class="w-2 h-2 rounded-full bg-primary-500"></span>
          Multi-Agent Systems
        </h3>
        <div class="grid gap-4 md:grid-cols-2">
          {papers.multiAgent.map((paper) => (
            <a
              href={paper.url}
              target="_blank"
              rel="noopener noreferrer"
              class="block p-5 bg-white dark:bg-slate-800 rounded-lg border border-slate-200 dark:border-slate-700 hover:border-primary-300 dark:hover:border-primary-700 transition-colors"
            >
              <h4 class="font-semibold text-slate-900 dark:text-white mb-1 flex items-start gap-2">
                {paper.title}
                <svg class="w-4 h-4 text-slate-400 flex-shrink-0 mt-0.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14"/>
                </svg>
              </h4>
              <p class="text-sm text-slate-500 dark:text-slate-400 mb-2">
                {paper.authors} ({paper.year}) &middot; {paper.venue}
              </p>
              <p class="text-sm text-slate-600 dark:text-slate-300">
                {paper.description}
              </p>
            </a>
          ))}
        </div>
      </div>

      <!-- RAG Papers -->
      <div class="mb-10">
        <h3 class="text-xl font-semibold text-slate-800 dark:text-slate-200 mb-4 flex items-center gap-2">
          <span class="w-2 h-2 rounded-full bg-primary-500"></span>
          RAG & Retrieval
        </h3>
        <div class="grid gap-4 md:grid-cols-2">
          {papers.rag.map((paper) => (
            <a
              href={paper.url}
              target="_blank"
              rel="noopener noreferrer"
              class="block p-5 bg-white dark:bg-slate-800 rounded-lg border border-slate-200 dark:border-slate-700 hover:border-primary-300 dark:hover:border-primary-700 transition-colors"
            >
              <h4 class="font-semibold text-slate-900 dark:text-white mb-1 flex items-start gap-2">
                {paper.title}
                <svg class="w-4 h-4 text-slate-400 flex-shrink-0 mt-0.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14"/>
                </svg>
              </h4>
              <p class="text-sm text-slate-500 dark:text-slate-400 mb-2">
                {paper.authors} ({paper.year}) &middot; {paper.venue}
              </p>
              <p class="text-sm text-slate-600 dark:text-slate-300">
                {paper.description}
              </p>
            </a>
          ))}
        </div>
      </div>

      <!-- Benchmarks Papers -->
      <div class="mb-10">
        <h3 class="text-xl font-semibold text-slate-800 dark:text-slate-200 mb-4 flex items-center gap-2">
          <span class="w-2 h-2 rounded-full bg-primary-500"></span>
          Benchmarks & Evaluation
        </h3>
        <div class="grid gap-4 md:grid-cols-2">
          {papers.benchmarks.map((paper) => (
            <a
              href={paper.url}
              target="_blank"
              rel="noopener noreferrer"
              class="block p-5 bg-white dark:bg-slate-800 rounded-lg border border-slate-200 dark:border-slate-700 hover:border-primary-300 dark:hover:border-primary-700 transition-colors"
            >
              <h4 class="font-semibold text-slate-900 dark:text-white mb-1 flex items-start gap-2">
                {paper.title}
                <svg class="w-4 h-4 text-slate-400 flex-shrink-0 mt-0.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14"/>
                </svg>
              </h4>
              <p class="text-sm text-slate-500 dark:text-slate-400 mb-2">
                {paper.authors} ({paper.year}) &middot; {paper.venue}
              </p>
              <p class="text-sm text-slate-600 dark:text-slate-300">
                {paper.description}
              </p>
            </a>
          ))}
        </div>
      </div>

      <!-- Safety Papers -->
      <div class="mb-10">
        <h3 class="text-xl font-semibold text-slate-800 dark:text-slate-200 mb-4 flex items-center gap-2">
          <span class="w-2 h-2 rounded-full bg-primary-500"></span>
          Safety & Alignment
        </h3>
        <div class="grid gap-4 md:grid-cols-2">
          {papers.safety.map((paper) => (
            <a
              href={paper.url}
              target="_blank"
              rel="noopener noreferrer"
              class="block p-5 bg-white dark:bg-slate-800 rounded-lg border border-slate-200 dark:border-slate-700 hover:border-primary-300 dark:hover:border-primary-700 transition-colors"
            >
              <h4 class="font-semibold text-slate-900 dark:text-white mb-1 flex items-start gap-2">
                {paper.title}
                <svg class="w-4 h-4 text-slate-400 flex-shrink-0 mt-0.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14"/>
                </svg>
              </h4>
              <p class="text-sm text-slate-500 dark:text-slate-400 mb-2">
                {paper.authors} ({paper.year}) &middot; {paper.venue}
              </p>
              <p class="text-sm text-slate-600 dark:text-slate-300">
                {paper.description}
              </p>
            </a>
          ))}
        </div>
      </div>
    </section>

    <!-- Frameworks Section -->
    <section id="frameworks" class="mb-16">
      <h2 class="text-2xl font-bold text-slate-900 dark:text-white mb-8">
        Frameworks & Libraries
      </h2>

      <!-- Orchestration -->
      <div class="mb-10">
        <h3 class="text-xl font-semibold text-slate-800 dark:text-slate-200 mb-4 flex items-center gap-2">
          <span class="w-2 h-2 rounded-full bg-blue-500"></span>
          Agent Orchestration
        </h3>
        <div class="grid gap-4 md:grid-cols-2 lg:grid-cols-4">
          {frameworks.orchestration.map((fw) => (
            <a
              href={fw.url}
              target="_blank"
              rel="noopener noreferrer"
              class="block p-5 bg-white dark:bg-slate-800 rounded-lg border border-slate-200 dark:border-slate-700 hover:border-primary-300 dark:hover:border-primary-700 transition-colors"
            >
              <h4 class="font-semibold text-slate-900 dark:text-white mb-1 flex items-center gap-2">
                {fw.name}
                <svg class="w-4 h-4 text-slate-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14"/>
                </svg>
              </h4>
              <p class="text-xs text-primary-600 dark:text-primary-400 mb-2">{fw.language}</p>
              <p class="text-sm text-slate-600 dark:text-slate-300">
                {fw.description}
              </p>
            </a>
          ))}
        </div>
      </div>

      <!-- Tooling -->
      <div class="mb-10">
        <h3 class="text-xl font-semibold text-slate-800 dark:text-slate-200 mb-4 flex items-center gap-2">
          <span class="w-2 h-2 rounded-full bg-green-500"></span>
          LLM Tooling
        </h3>
        <div class="grid gap-4 md:grid-cols-3">
          {frameworks.tooling.map((fw) => (
            <a
              href={fw.url}
              target="_blank"
              rel="noopener noreferrer"
              class="block p-5 bg-white dark:bg-slate-800 rounded-lg border border-slate-200 dark:border-slate-700 hover:border-primary-300 dark:hover:border-primary-700 transition-colors"
            >
              <h4 class="font-semibold text-slate-900 dark:text-white mb-1 flex items-center gap-2">
                {fw.name}
                <svg class="w-4 h-4 text-slate-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 6H6a2 2 0 00-2 2v10a2 2 0 002 2h10a2 2 0 002-2v-4M14 4h6m0 0v6m0-6L10 14"/>
                </svg>
              </h4>
              <p class="text-xs text-primary-600 dark:text-primary-400 mb-2">{fw.language}</p>
              <p class="text-sm text-slate-600 dark:text-slate-300">
                {fw.description}
              </p>
            </a>
          ))}
        </div>
